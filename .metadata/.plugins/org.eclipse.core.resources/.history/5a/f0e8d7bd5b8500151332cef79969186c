package com.radojko.fibonacci;

import static org.junit.Assert.*;
import org.junit.Assert;
import org.junit.Test;

public class fibonacciTestCase {

	@Test
	public void test0() {
		Fibonacci fib = new Fibonacci();
		Assert.assertEquals(1, fib.finbonacci(0));
	}	
	
	@Test
	public void test1() {
		Fibonacci fib = new Fibonacci();
		Assert.assertEquals(1, fib.finbonacci(1));
	}	
	
	@Test
	public void test3() {
		Fibonacci fib = new Fibonacci();
		Assert.assertEquals(8, fib.finbonacci(3));
	}
	
	@Test
	public void test15() {
		Fibonacci fib = new Fibonacci();
		Assert.assertEquals(8, fib.finbonacci(15));
	}	

}
