package com.radojko.fibonacci;

import javax.jws.WebService;

@WebService
public class Fibonacci {
	public Integer finbonacci(int n){
		int[] fibArray;
		
		for (int i = 0; i < n; i++) {
			if (i == 0) {
				fibArray[i] = 1;
			}
			if (i == 1) {
				fibArray[i] = 1;
			}
			if (i > 1) {
				fibArray[i] = fibArray[i - 1] + fibArray[i - 2]; 
			}
		}
		return fibArray[n - 1];
	}
}
